{
  "kind": "collectionType",
  "collectionName": "material_balances",
  "info": {
    "singularName": "material-balance",
    "pluralName": "material-balances",
    "displayName": "Material Balance",
    "description": "Anyagegyenlegek"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "total_picked_up": {
      "type": "json"
    },
    "total_used": {
      "type": "json"
    },
    "balance": {
      "type": "json"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "surplus",
        "balanced",
        "deficit"
      ],
      "default": "balanced"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user"
    },
    "company": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::company.company",
      "inversedBy": "material_balances"
    },
    "material": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::material.material"
    }
  }
}